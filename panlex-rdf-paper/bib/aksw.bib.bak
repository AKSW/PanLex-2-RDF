@BOOK{CLP-2010,
  title = {Catalogus Professorum Lipsiensis -- Konzeption, technische Umsetzung
	und Anwendungen für Professorenkataloge im Semantic Web},
  publisher = {Leipziger Informatik-Verbund (LIV)},
  year = {2010},
