@MISC{d2rserver,
  author = {Bizer, C. and Cyganiak, R.},
  title = {{D2R} Server - Publishing Relational Databases on the Semantic Web},
  howpublished = {Poster at ISWC},
  year = {2006},
  abstract = {D2R Server is a tool for publishing the content of relational databases
	on the Semantic Web. Database content is mapped to RDF by a declar-
	ative mapping which specifies how resources are identified and how
	property values are generated from database content. Based on this
	mapping, D2R Server allows Web agents to retrieve RDF and XHTML representations
	of resources and to query non-RDF databases using the SPARQL query
	lan- guage over the SPARQL protocol. The gener- ated representations
	are richly interlinked on RDF and XHTML level in order to enable
	browsers and crawlers to navigate database content.},
  added-at = {2010-02-26T01:12:39.000+0100},
  biburl = {http://www.bibsonomy.org/bibtex/282f2b0008fd60e33ea33c402d8ec0315/alexjdl},
